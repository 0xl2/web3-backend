{
  "name": "web3-sdk",
  "version": "1.0.0",
  "description": "A starter Web3 SDK project for quickly building REST APIs using Node.js, Express, and Mongoose.",
  "main": "src/index.ts",
  "repository": "https://github.com/abe-sinjiro/web3-backend.git",
  "author": "Web3 backend",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prestart": "cp .env ./dist/.env && cp fireblocks_secret.key ./dist/fireblocks_secret.key",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "nodemon",
    "build": "tsc && tsc-alias",
    "test": "jest -i --colors --verbose --detectOpenHandles --coverage",
    "test:single": "jest -i --colors --verbose --detectOpenHandles -- ",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "browser": {
    "moralis": false,
    "react-moralis": false
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.6.1",
    "@imtbl/imx-sdk": "^1.20.1",
    "@mailchimp/mailchimp_transactional": "^1.0.47",
    "@types/lodash": "^4.14.182",
    "aws-sdk": "^2.1132.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "fireblocks-sdk": "^2.2.0",
    "google-auth-library": "^8.1.1",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "mongoose-sequence": "^5.3.1",
    "moralis": "^1.10.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.0",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "ts-migrate": "^0.1.28",
    "ts-migrate-plugins": "^0.1.28",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-sequence": "^3.0.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.2",
    "coveralls": "^3.0.7",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "i": "^0.3.7",
    "jest": "^28.0.3",
    "lint-staged": "^11.0.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "npm": "^8.9.0",
    "prettier": "^2.0.5",
    "supertest": "^6.0.1",
    "ts-jest": "^28.0.1",
    "tsc-alias": "^1.6.7"
  }
}
